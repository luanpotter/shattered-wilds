---js
{
    permalink: "/{{ pagination.items[0].url }}/index.html",
    pagination: {
        data: "traits",
        alias: "lexicon",
        size: 1,
    }
}
---

{% include 'wiki-header.liquid' %}

{{ lexicon.content | mdb }}

<h3>{{ lexicon.target }}s with the {{ lexicon.title }} Trait</h3>
<ul>
    {% for item in wiki.byGroupByTrait[lexicon.target][lexicon.slug] %}
        <li>{% item item.slug %}</li>
    {% endfor %}
</ul>